{
    "manifest_version": 3,
    "name": "You Tube playlist",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": ["activeTab","scripting"],
    "action": {
      "default_popup": "./index.html",
      "default_icon": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
      }
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
  }